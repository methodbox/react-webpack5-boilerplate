{
  "name": "react-webpack-boilerplate",
  "version": "1.0.0",
  "description": "React + Webpack 5 boilerplate w/support for CSS, images and fonts. Includes React Styleguidist.",
  "main": "index.js",
  "scripts": {
    "build-dev": "webpack -d --mode development",
    "build": "rm -rf ./dist/bundle.js && webpack -p --mode production",
    "start": "webpack serve --config ./webpack.config.js --mode development --open"
  },
  "author": "Chris A. Shaffer",
  "license": "MIT",
  "dependencies": {
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-dom": "^17.0.1",
    "react-styleguidist": "^11.1.5",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@hot-loader/react-dom": "^17.0.1",
    "babel-loader": "^8.2.2",
    "css-loader": "^4.3.0",
    "file-loader": "^6.2.0",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^1.3.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.20.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
